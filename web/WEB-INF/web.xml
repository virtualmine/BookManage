<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>BookManage</display-name>
  <welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>UserServ</servlet-name>
  	<servlet-class>action.UserServ</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserServ</servlet-name>
  	<url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>BooksServ</servlet-name>
  	<servlet-class>action.BooksServ</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>BooksServ</servlet-name>
  	<url-pattern>*.da</url-pattern>
  </servlet-mapping>
</web-app>